# ðŸ“œ .env (Do NOT commit this file to GitHub)

# Database Configuration (Use DB_URL instead of separate variables)
DB_URL=postgresql://your_db_user:your_secure_password@your_db_host:5432/your_db_name

# Hyperledger Indy Wallet & Ledger Configuration
INDY_WALLET_ID=trustid_wallet
INDY_WALLET_KEY=your_wallet_key
ISSUER_DID=your_issuer_did

# API Keys & Secrets (if applicable)
API_KEY=your_api_key

# Indy Pool Configuration (update with the correct path to your genesis transaction file)
INDY_POOL_NAME=your_pool_name
INDY_GENESIS_FILE_PATH=/path/to/sovrin_stagingnet.txn

# Logging Configuration (Optional)
LOG_LEVEL=INFO
